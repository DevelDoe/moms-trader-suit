{
    "name": "Moms_Trader_Tools",
    "version": "0.10.1",
    "description": "Moms Trader Tools is a comprehensive suite designed to enhance the trading experience for both novice and experienced day traders. With a focus on efficiency and user-friendliness, this toolset aims to streamline your trading process and provide a competitive edge.",
    "main": "src/main/main.js",
    "scripts": {
        "start": "electron .",
        "package": "electron-packager . Moms_Trader_Suit --platform=win32 --arch=x64 --out=dist --overwrite",
        "package-all": "electron-packager . Moms_Trader_Suit --all --out=dist --overwrite",
        "package-mac": "electron-packager . Moms_Trader_Suit --platform=darwin --arch=x64 --out=dist --overwrite",
        "package-linux": "electron-packager . Moms_Trader_Suit --platform=linux --arch=x64 --out=dist --overwrite"
    },
    "author": "Morgan A Ray",
    "license": "MIT",
    "dependencies": {
        "chokidar": "^4.0.3",
        "ignore": "^7.0.3"
    },
    "devDependencies": {
        "electron": "^26.1.0",
        "electron-packager": "^17.1.2"
    },
    "build": {
        "files": [
            "src/**/*",
            "assets/**/*"
        ]
    }
}
